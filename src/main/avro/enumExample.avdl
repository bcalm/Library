protocol Book {
	record Book {
		int id;
		boolean isBorrowed;
		union{null, string} name;
		union{null, string} defaultValue = null;
		array<string> authors;
		EnumExample enumExample;
		ExtraRecord extraRecord;
	}

	enum EnumExample {
		Value1, Value2, Value3
	}

	record ExtraRecord {
		string record1;
		string record2;
	}
}